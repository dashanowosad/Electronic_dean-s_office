<Window x:Class="Diploma_v2.AboutOneComittee_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Diploma_v2"
        mc:Ignorable="d"
        Title="Просмотр комиссии" Height="300" Width="500">
    <Grid>
        
        <WrapPanel HorizontalAlignment="Center" VerticalAlignment="Center" Loaded="LoadText">
            <TextBlock x:Name="Info" TextWrapping="Wrap"  Text="Комиссия" FontSize="14" Margin="10 -100 10 10"/>
        </WrapPanel>
     
    </Grid>
</Window>
